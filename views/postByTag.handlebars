<div class="tagAndProfile d-flex align-items-center  justify-content-around">
<button type="button" class="newPostBtn rounded py-1 px-2 fs-3" data-bs-toggle="modal" data-bs-target="#exampleModal">
    New Post
  </button>
    <select class="tags form-select tagDropdown my-auto"  aria-label="Default select example" name="tags">
                   <option disabled selected value>Filter by Hashtag</option>
                   <option value="all" name="all">all</option>
                    {{#each tag}}
                
                    <option value={{id}} >#{{name}}</option>
                    {{/each}}
                </select>
</div>
<div class="container d-flex flex-wrap justify-content-center">
{{#each post}}
  {{>posts}}
{{/each}}

</div>
  {{>newPost}}

</div>
