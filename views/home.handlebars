<div class="navbar d-flex text-white justify-content-around mt-2 flex-wrap">
  <button type="button" class="newPostBtn bg-transparent btn-primary fs-2" data-bs-toggle="modal" data-bs-target="#exampleModal">
    Add New Post
  </button>
  <div>
    <h1 class="projectName mt-2">Project 2</h1>
  </div>
  <a class="text-decoration-none text-white fs-2 mt-2" href="#" id="logout">Logout</a>
</div>

{{>newPost}}


<div class="tagAndProfile align-items-center d-flex justify-content-around">
<a href="/user/{{userId}}" class="text-white fs-2 ">My Profile</a>
  <select class="tags tagDropdown" name="tags">
    <option disabled selected value>---Tags---</option>
    <option value="all" name="all">all</option>
    {{#each tag}}

    <option value={{id}}>{{name}}</option>
    {{/each}}
  </select>
</div>
<div class="container d-flex flex-wrap justify-content-center">
  {{#each post}}
  {{>posts}}
  {{/each}}

</div>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
  integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
<script src="js/post.js"></script>
</div>